{% trans_default_domain 'expertise' %}

{% extends 'ExpertiseBundle:Admin:layout.html.twig' %}

{% block title %}{{ parent() }} - {{ "expertise.partner.create.title"|trans|capitalize }} {% endblock %}

{% block breadcrumbmodule %}
	{{ parent() }}
    <a href="{{ url('puzzle_admin_expertise_partner_list') }}">{{ "expertise.partner.list.title"|trans|capitalize }}</a>
	<i class="fa fa-chevron-right"></i>
	<em>{{ "expertise.partner.create"|trans|capitalize }}</em>
{% endblock %}

{% block content %}
    <div id="page_content_inner">
        {% include 'ExpertiseBundle:Admin:form_partner.html.twig' with {'form': form} %}
    </div>
{% endblock %}
